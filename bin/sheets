#!/usr/bin/env node
// Get pages
// watchy -w src -- bash -c "cat ../payloads/gburger/gburger-hamburger.bin | bin/sheets 2>&1 | simple-stacktrace"

var VirtualPrinter = require('../src');
var vp = new VirtualPrinter();

process.stdin.on('data', function(buf) {
  vp.getSheets(buf, function(sheets) {
    console.log('\ngot '+sheets.length+' sheets\n', sheets);
  });
});
